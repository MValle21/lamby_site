<div id="disqus_thread"></div>
<script>
  var disqus_config = function() {
    this.page.url = <%= "#{request.base_url}#{request.path_info}".html_safe.inspect %>;
    this.page.identifier = <%= request.path_info %>.html_safe.inspect;
    this.page.title = <%= doc_name(params[:id]) %>.html_safe.inspect;
  };
  (function() {
  var d = document, s = d.createElement('script');
  s.src = 'https://lamby.disqus.com/embed.js';
  s.setAttribute('data-timestamp', +new Date());
  (d.head || d.body).appendChild(s);
  })();
</script>
